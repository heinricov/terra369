generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dth22 {
  id          Int      @id @default(autoincrement())
  unit_name   String
  suhu        Float
  kelembapan  Float
  created_at  DateTime @default(now())
}

  